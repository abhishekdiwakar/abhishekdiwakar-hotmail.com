<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
        <pattern>
          <pattern>%d{dd-MM-yyyy HH:mm:ss.SSS} %-5level %msg ## \(%mdc\) %logger{36} %n</pattern>
        </pattern>
      </encoder>
    </appender>
    <root>
      <appender-ref ref="STDOUT"/>
    </root>
  <root>
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>